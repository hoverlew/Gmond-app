.fllter-search-box{
  width:100%;
  height:250rpx;
  border-top:3rpx solid #eeeeee;
  border-bottom: 1rpx solid #dddddd;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size:30rpx;
}
.search-content{
  width:70%;
  height:150rpx;
}
.search-content label{
  width:200rpx;
  text-align: left;
  height:50rpx;
  line-height: 50rpx;
}
.search-box{
  display: flex;
  flex-direction: row;  
  margin-top:10rpx;
}
.search-box input{
  width:220rpx;
  height:60rpx;
  border:1rpx solid #999999;
  text-align: center;
  overflow:unset;
}
.search-box text{
  margin:10rpx 30rpx;
  color:#999999;
}
.filter-enter{
  width:100%;
  height:80rpx;
  display: flex;
  flex-direction: row;
  position: absolute;
  bottom: 0;
  left:0;
}
.filter-enter view{
  width:50%;
  font-size:30rpx;
  border-radius: 0;
  height:79rpx;
  line-height:79rpx;
  text-align:center;
}
.btn-reset{
  background-color:#ffffff;
  color:#f6827a;
  border-top:1rpx solid #dddddd;
}
.btn-enter{
  background-color:#f6827a;
  color:#ffffff;
  border-top:1rpx solid #f6827a;
}