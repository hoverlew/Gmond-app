.li-btn{
  width:25%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.li-btn button{
  font-size:28rpx;
  background:unset;
  color: #fb9e9d;
  margin:0;
  justify-content:flex-end;
  text-align: right;
  padding:10rpx;
}