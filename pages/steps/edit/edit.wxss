page {
  background: #f2ebe3;
}

.cart {
  padding: 100px 0 0 0;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #999;
}

.cart image {
  width: 66px;
  height: 66px;
  margin-bottom: 20px;
}

.goods {
  background: #fff;
  border-top: 1px solid #ddd;
  margin-bottom: 10px;
  padding: 10px 10px 0 10px;
  display: flex;
  flex-direction: column;
  height: auto;
  padding-bottom: 60rpx;
}

.goods-up {
  display: flex;
  height: auto;
}

.goods-dn {
  display: flex;
  flex-direction: column;
}

.goods-dn view {
  border-bottom: 1px solid #ddd;
  height: auto;
  line-height: 65rpx;
  margin-top: 5rpx;
}

.goods-dn text {
  font-size: 30rpx;
  width: auto;
  height: 45rpx;
  line-height: 45rpx;
  border-radius: 5rpx;
  padding: 5rpx 10rpx;
  text-align: center;
  float: left;
  position: relative;
  left: -50rpx;
}

text.material-title {
  margin: 0 2rpx 10rpx 20px;
}

.goods-dn .material-title, .goods-dn .weight-title {
  border: none;
  color: #333;
  background: none;
}

.goods-up image {
  width: 80px;
  height: 80px;
  border: 1px solid #ddd;
}

.goods-up .good-cont {
  display: flex;
  flex: 1;
  flex-direction: column;
  color: #555;
  font-size: 14px;
  padding: 10rpx 0 0 35rpx;
  height: 100px;
}

.goods-up .good-cont .goods-navigator {
  display: flex;
  padding-bottom: 5rpx;
}

.goods-up .good-cont .good-price {
  display: flex;
  flex-direction: column;
  flex: 3;
}

.goods-up .good-cont .good-price .price {
  font-size: 30rpx;
  color: #ec5151;
}

.stock {
  display: flex;
  flex-direction: row;
}

.pro-right-num {
  justify-content: center;
  align-items: center;
  width: 150rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  display: flex;
  border: 1rpx solid #ddd;
  margin-top: 45rpx;
}

.pro-right-dec {
  justify-content: center;
  align-items: center;
  width: 50rpx;
  height: 45rpx;
  font-weight: bold;
  font-size: 32rpx;
}

.pro-right-label {
  justify-content: center;
  align-items: center;
  width: 50rpx;
  height: 45rpx;
  border-left: 1rpx solid #ddd;
  font-size: 26rpx;
}

.pro-right-add {
  justify-content: center;
  align-items: center;
  width: 50rpx;
  height: 45rpx;
  font-weight: bold;
  border-left: 1rpx solid #ddd;
  font-size: 32rpx;
}

.pro-right-stock {
  height: 45rpx;
  line-height: 45rpx;
  margin-top: 45rpx;
  padding-left: 180rpx;
}

.pro-right-stock text text {
  color: #f6827a;
  font-size: 25rpx;
  padding: 0 10rpx;
}

.goods .good-cont .good-price .btn-box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.goods .good-cont .good-price .btn-box image {
  width: 23px;
  height: 23px;
  border: 0;
  margin: 0;
}

.goods .good-cont .good-price .btn {
  display: flex;
  flex-direction: row;
}

.goods .good-cont .good-price .btn input {
  margin: 0;
  width: 40px;
  text-align: center;
  border: 1px solid #eee;
  font-size: 16px;
  height: 28px;
}

.goods .good-cont .good-price .btn button {
  margin: 0;
}

.total {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: absolute;
  bottom: 0;
  width: 100%;
}

.total .total_js {
  color: #fff;
  background: #ec5151;
  margin: 0;
  border-radius: 0;
  width: 100%;
  line-height: 90rpx;
}

view.material-list {
  width: auto;
  height: auto;
  float: left;
  border-bottom: none;
}

.title {
  padding: 10rpx 20rpx;
  margin: 10rpx 0;
  border-left: 4rpx solid #ccc;
}

/*全部属性的主盒子*/

.commodity_attr_list {
  background: #fff;
  padding: 0 20rpx;
  font-size: 26rpx;
  overflow: hidden;
  width: 100%;
  margin-top: 30rpx;
}

/*每组属性的主盒子*/

.attr_box {
  width: 100%;
  overflow: hidden;
  border-bottom: 1rpx solid #ececec;
}

/*属性名*/

.attr_name {
  width: 20%;
  float: left;
  padding: 15rpx 0;
}

/*属性值*/

.attr_value_box {
  width: 80%;
  float: left;
  padding: 15rpx 0;
  overflow: hidden;
}

/*每个属性值*/

.attr_value {
  float: left;
  padding: 3rpx 20rpx;
  margin: 0 10rpx;
  border: 1rpx solid #ececec;
  margin-bottom: 15rpx;
}

/*每个属性选中的当前样式*/

.attr_value_active {
  background: #f6827a;
  color: #fff;
  padding: 3rpx 20rpx;
}

/*禁用属性*/

.attr_value_disabled {
  color: #ccc;
}

/*button*/

.btn-area {
  margin: 1.17647059em 15px 0.3em;
}

.btn {
  margin-top: 15px;
  background-color: #fc0;
  color: #fff;
}

.btn:first-child {
  margin-top: 0;
}
